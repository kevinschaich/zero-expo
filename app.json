{
    "expo": {
        "name": "zero-expo",
        "slug": "zero-expo",
        "version": "1.0.0",
        "scheme": "zero-expo",
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-router", "expo-sqlite"],
        "experiments": {
            "typedRoutes": true,
            "tsconfigPaths": true
        },
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.zero-expo.zero-expo"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.zero-expo.zero-expo"
        }
    }
}
